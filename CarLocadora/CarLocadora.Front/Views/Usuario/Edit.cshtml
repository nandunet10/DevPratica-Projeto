@model CarLocadora.Modelo.Modelos.UsuarioModel

@{
    ViewData["Title"] = "Edit";
}

<h4 >Editar usuario</h4>
<hr />
<form asp-action="Edit">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <partial name="_Formulario" />
    <div class="col-md-5">
        <div class="form-group">
            <input asp-for="DataInclusao" class="form-control" hidden />
            <span asp-validation-for="DataInclusao" class="text-danger"></span>
        </div>
    </div>
</form>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
