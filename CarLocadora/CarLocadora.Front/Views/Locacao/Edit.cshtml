@model CarLocadora.Modelo.Modelos.LocacoesModel

@{
    ViewData["Title"] = "Edit";
}

<h4>Editar Locação</h4>
<hr class="bg-white" />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <input asp-for="Id" type="hidden" />
            <input asp-for="DataInclusao" type="hidden" />
            <input asp-for="DataAlteracao" type="hidden" />
            <partial name="_Formulario" />
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
