@model CarLocadora.Modelo.Modelos.ClienteModel

@{
    ViewData["Title"] = "Edit";
}

<h4 >Editar Cliente</h4>

<br />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <partial name="_Formulario" />
            <div class="col-md-5">
                <div class="form-group">
                    <input asp-for="DataInclusao" class="form-control" hidden />
                    <span asp-validation-for="DataInclusao" class="text-danger"></span>
                </div>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
